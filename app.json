{
    "name": "aps-revit-rcw-migrate-automation",
    "description": "This sample demostrated how to batch migrate Revit Cloud Worksharing (RCW) models from one BIM 360/ACC project to another using Revit Automation API.",
    "repository": "https://github.com/autodesk-platform-services/aps-revit-rcw-migrate-automation",
    "logo": "https://cdn.autodesk.io/favicon.ico",
    "keywords": [
        "autodesk",
        "aps",
        "revit automation",
        "rcw",
        "bim 360",
        "acc"
    ],
    "env": {
        "APS_CLIENT_ID": {
            "description": "APS Client ID"
        },
        "APS_CLIENT_SECRET": {
            "description": "APS Client Secret"
        },
        "APS_CALLBACK_URL": {
            "description": "Callback URL of your APS app, required for 3-legged OAuth",
            "value": "https://<<YOUR HEROKU APP NAME HERE>>.herokuapp.com/callback/oauth"
        },
        "APS_WEBHOOK_URL": {
            "description": "Callback URL of your webhook",
            "value": "http://<<YOUR HEROKU APP NAME HERE>>.herokuapp.com/callback/designautomation"
        }
    },
    "website": "https://developer.autodesk.com/",
    "success_url": "/"
}